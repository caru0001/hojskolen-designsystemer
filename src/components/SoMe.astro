---
import { Image } from "astro:assets";
import some1 from "../assets/images/some1.webp";
import some2 from "../assets/images/some2.webp";
import some3 from "../assets/images/some3.webp";
import some4 from "../assets/images/some4.webp";
import some5 from "../assets/images/some5.webp";
import some6 from "../assets/images/some6.webp";
import some7 from "../assets/images/some7.webp";
import some8 from "../assets/images/some8.webp";
import some9 from "../assets/images/some9.webp";
import some10 from "../assets/images/some10.webp";
---

<div class="text-Black-500 gap-y-16">
  <h1 class="text-h1 font-code">FØLG MED PÅ</h1>
  <h2 class="text-subheading font-sans">
    <a href="https://www.instagram.com" target="_blank" class="border-b-small border-Black-500">Instagram</a> og
    <a href="https://www.tiktok.com" target="_blank" class="border-b-small border-Black-500">TikTok</a>
  </h2>

  <!-- Grid Layout -->
  <div class="grid grid-cols-5 space-x-12 gap-y-12 mt-48">
    <div class="flex justify-center">
      <Image src={some1} alt="Image 1" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some2} alt="Image 2" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some3} alt="Image 3" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some4} alt="Image 4" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some5} alt="Image 5" class="w-full h-56 rounded-large" />
    </div>

    <div class="flex justify-center">
      <Image src={some6} alt="Image 6" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some7} alt="Image 7" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some8} alt="Image 8" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some9} alt="Image 9" class="w-full h-56 rounded-large" />
    </div>
    <div class="flex justify-center">
      <Image src={some10} alt="Image 10" class="w-full h-56 rounded-large" />
    </div>
  </div>
</div>
